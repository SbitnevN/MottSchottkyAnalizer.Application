<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryBrush" Color="#77A1A6"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="#1D6A73"/>
    <SolidColorBrush x:Key="AccentBrush" Color="#ED1E8B"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="#FFF1FAFB"/>
    <SolidColorBrush x:Key="TextBrush" Color="#FF1D3557"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#FF1D3557"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="#FFEF476F"/>
    <SolidColorBrush x:Key="SuccessBrush" Color="#FF06D6A0"/>
    
    <SolidColorBrush x:Key="White" Color="#F2F2F2"/>
    <SolidColorBrush x:Key="Black" Color="#1F1F1F"/>

</ResourceDictionary>